<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="Description" content="Oijazz ist ein Verlag">
    <link rel="shortcut icon" href="/images/favicon.ico">
    <link rel="stylesheet" href="/css/styles.css">
    <link rel="preconnect" href="//spreadsheets.google.com">
    <title>{{ title }}</title>
    {% set js %}
<!-- {% include "js/events.js" %} -->
    {% endset %}
    <script async>{{ js | jsmin | safe }}</script>
  </head>
  <body {% if bodyClass %}class="{{ bodyClass }}"{% endif %}>

    <div class="container{% if smallContainer %} container--small {% endif %}">
      {% include "header.njk" %}
      <main>
        {{ content | safe }}
      </main>
      {% include "footer.njk" %}
    </div>

  </body>
</html>



